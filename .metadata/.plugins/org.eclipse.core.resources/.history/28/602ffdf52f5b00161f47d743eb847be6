import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
import java.io.PrintWriter;

public class Testing {
	public static void main(String[] args){
		
		int[][][] test = {
				// Group 1
				{
					// Team 1
					{ 0,1,2,3,4},
					// Team 2
					{ 5,6,7}
				}
		};
		
		System.out.println("test.length prints:"+test.length);
		System.out.println("test[0].length prints:"+test[0].length);
		System.out.println("test[0][0].length prints:"+test[0][0].length);

	}

}
