import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
import java.io.PrintWriter;
import java.util.ArrayList;

public class Testing {
	public static void main(String[] args){
		
		int[][][] test = {
				// Group 1
				{
					// Team 1
					{ 0,1,2,3,4},
					// Team 2
					{ 5,6,7}
				}
		};
		
		System.out.println("test.length prints:"+test.length); // 1
		System.out.println("test[0].length prints:"+test[0].length);// 2
		System.out.println("test[0][0].length prints:"+test[0][0].length); // 5
		
		ArrayList<Integer> testInt = new ArrayList<Integer>();
//		testInt.add(2);
		System.out.println("testInt.size() prints:"+testInt.size());
		System.out.println("testInt(0) prints: "+ testInt.get(0));

	}

}
